{
	"$schema": "https://schemas.wp.org/wp/6.0/theme.json",
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "30rem",
			"wideSize": "40rem"
		},
		"color": {
			"background": false,
			"link": true,
			"defaultPalette": false,
			"custom": false,
			"customGradient": false,
			"gradients": [],
			"palette": [
				{
					"color": "#333",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "light"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"IBM Plex Serif\", serif",
					"name": "IBM Plex Serif",
					"slug": "ibm-plex-serif",
					"fontFace": [
						{
							"fontFamily": "IBM Plex Serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-serif-v15-latin-regular.woff2",
								"file:./assets/fonts/ibm-plex-serif-v15-latin-regular.woff"
							]
						},
						{
							"fontFamily": "IBM Plex Serif",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-serif-v15-latin-700.woff2",
								"file:./assets/fonts/ibm-plex-serif-v15-latin-700.woff"
							]
						},
						{
							"fontFamily": "IBM Plex Serif",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-serif-v15-latin-italic.woff2",
								"file:./assets/fonts/ibm-plex-serif-v15-latin-italic.woff"
							]
						}
					]
				}
			]
		},
		"blocks": {
			"core/group": {
				"color": {
					"background": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--dark)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibm-plex-serif)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "currentColor"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header-h1",
			"title": "Header (h1)",
			"area": "header"
		},
		{
			"name": "header-p",
			"title": "Header (p)",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}